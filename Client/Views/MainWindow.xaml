<Window x:Class="Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Client"
        xmlns:vm="clr-namespace:Client.ViewModels"        mc:Ignorable="d"
        Title="{Binding Title}" Height="450" Width="800" Background="#173467">
    <Window.DataContext>
        <vm:MainWindowViewModel/>
    </Window.DataContext>
    <StackPanel>
        <TextBox HorizontalAlignment="Left" Margin="16,18,0,0" TextWrapping="Wrap" 
                 Text="{Binding Enter, UpdateSourceTrigger=PropertyChanged}"
                 VerticalAlignment="Top" Width="120"/>
        <Label Content="{Binding Enter}" HorizontalAlignment="Left"
               Margin="15" Height="29" Width="342" RenderTransformOrigin="-0.298,0.968"/>
    </StackPanel>
</Window>
